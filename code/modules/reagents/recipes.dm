/datum/chemical_reaction/ui_data()
	var/list/dat = list()
	if(re69uired_reagents)
		dat69"reagents"69 = list()
		for(var/id in re69uired_reagents)
			dat69"reagents"69 += list(list("type" = get_reagent_type_by_id(id), "reagent" = get_reagent_name_by_id(id), "parts" = "69re69uired_reagents69id6969 part\s"))
	if(catalysts)
		dat69"catalyst"69 = list()
		for(var/id in catalysts)
			dat69"catalyst"69 += list(list("type" = get_reagent_type_by_id(id), "reagent" = get_reagent_name_by_id(id), "units" = catalysts69id69))

	dat69"minimum_temperature"69 =69inimum_temperature
	if(maximum_temperature != INFINITY)
		dat69"maximum_temperature"69 =69aximum_temperature

	dat69"result_amount"69 = "69result_amount69 part\s"
	return dat
