{{if data.logined}}
	{{if data.browsing_logs}}
		<div class="statusDisplay" style="overflow: auto;">
			<tr><td>{{:helper.link('Back', null, {'back' : 1})}}</td></tr>
			<tr>
				<th><b>Timestamp</b></th>
				<th><b>Target</b></th>
				<th><b>Reason</b></th>
				<th><b>Value</b></th>
				<th><b>Terminal</b></th>
			</tr>
			{{for data.account_logs}}
				<tr>
					<td>{{:value.date}} {{:value.time}}</td>
					<td>{{:value.target_name}}</td>
					<td>{{:value.purpose}}</td>
					<td>{{:value.amount}}</td>
					<td>{{:value.source_terminal}}</td>
				</tr>
			{{/for}}
		</div>
	{{else}}
		<div align="center" style="margin-top: 15px;">
			<div class="item"><b>Access authorized</b></div>
			<div class="item">Logged as: {{:data.account_owner}}</div>
			<table>
				<tr>
					<td><span class='white'>Current balance: {{:data.account_balance}}</span></td>
					{{if !data.is_department_account}}
						<td><span class='white'>Hourly wage: {{:data.account_wage}}</span></td>
						<td><span class='white'>Employer: {{:data.account_alignment}}</span></td>
						<td>{{:helper.link('Resign ', null, {'resign' : 1})}}</td>
					{{/if}}
					<td>{{:helper.link('Logs', null, {'logs' : 1})}}</td>
					<td>{{:helper.link('Transfer', null, {'transfer' : 1})}}</td>
					<td>{{:helper.link('Log out', null, {'log_out' : 1})}}</td>
				</tr>
			</table>
		</div>
	{{/if}}
{{else}}
	{{if data.login_failed}}
		<div align="center" style="margin-top: 15px;">
			<div class="item"><b>An error has occurred</b></div>
			<div class="item">Invalid Credentials.</div>
			<table>
				<tr>
					<td>{{:helper.link('Back', null, {'back' : 1})}}</td>
				</tr>
			</table>
		</div>
	{{else}}
	<div align="center" style="margin-top: 15px;">
		<div class='item'>
			<div style="font-size: x-large;font-weight: bold;">TaxQuickly</div>
			<div style="font-style: italic;">Managing your money since 2559</div>
		</div>
		<div class="item">
			<div class="itemLabel">
				Options:
			</div>
			<div class="itemContent">
				{{:helper.link('Enter Tax ID', null, {'enter_login' : 1})}}
				{{:helper.link('Log In', null, {'log_in' : 1})}}
			</div>
		</div>
	</div>
	{{/if}}
{{/if}}